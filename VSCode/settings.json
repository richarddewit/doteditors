{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "valeryanm.vscode-phpsab"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "auto.languages": {
    "crystal": {
      "stringWrapper": "\"",
      "symbol": "#"
    }
  },
  "beautify.language": {
    "css": [
      "css",
      "scss"
    ],
    "html": [
      "htm",
      "html",
      "blade"
    ],
    "js": {
      "filename": [
        ".jshintrc",
        ".jsbeautify"
      ],
      "type": [
        "javascript",
        "json"
      ]
    }
  },
  "crystal-lang.completion": true,
  "crystal-lang.hover": true,
  "crystal-lang.implementations": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "docker-explorer.enableTelemetry": false,
  "docker-explorer.showAzureRegistries": false,
  "docker-explorer.showDockerHubTreeView": false,
  "docker-explorer.showSuggestedDockerImages": false,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "phase",
  "editor.find.autoFindInSelection": true,
  "editor.folding": false,
  "editor.fontFamily": "'Jetbrains Mono', 'Fira Code Retina', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "off",
  "editor.minimap.renderCharacters": false,
  "editor.quickSuggestions": false,
  "editor.quickSuggestionsDelay": 200,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "Procfile*": "yaml"
  },
  "files.autoSave": "onWindowChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/._*": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/_backup": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "html-css-class-completion.enableEmmetSupport": true,
  "html.format.wrapAttributes": "preserve-aligned",
  "html.format.wrapLineLength": 180,
  "importMagic.maxColumns": 88,
  "javascript.commonjs.resolve.extensions": [
    ".js",
    ".vue"
  ],
  "javascript.commonjs.resolve.languages": [
    "javascript",
    "javascriptreact",
    "vue",
    "vue-html"
  ],
  "nginx-conf-hint.syntax": "sublime",
  "npm.packageManager": "yarn",
  "phpcs.executablePath": "/home/richard/.config/composer/vendor/bin/phpcs",
  "phpsab.executablePathCBF": "/home/richard/.config/composer/vendor/bin/phpcbf",
  "phpsab.executablePathCS": "/home/richard/.config/composer/vendor/bin/phpcs",
  "prettier.trailingComma": "es5",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "python.languageServer": "Pylance",
  "python.linting.banditEnabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.pylintArgs": [
    "--rcfile=pyproject.toml"
  ],
  "python.sortImports.path": "/usr/bin/isort",
  "python.venvPath": "$WORKON_HOME",
  "search.smartCase": true,
  "svelte.plugin.typescript.enable": false,
  "svgviewer.enableautopreview": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.linuxExec": "urxvt",
  "terminal.integrated.fontSize": 12,
  "update.mode": "manual",
  "vim.easymotion": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    },
    {
      "before": [
        ","
      ],
      "commands": [
        "vspacecode.space",
        {
          "args": "m",
          "command": "whichkey.triggerKey"
        }
      ]
    }
  ],
  "vim.statusBarColorControl": false,
  "vim.statusBarColors.insert": "#83a598",
  "vim.statusBarColors.normal": "#b8bb26",
  "vim.statusBarColors.visual": "#fb4934 ",
  "vim.statusBarColors.visualblock": "#d3869b",
  "vim.statusBarColors.visualline": "#fabd2f",
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    },
    {
      "before": [
        ","
      ],
      "commands": [
        "vspacecode.space",
        {
          "args": "m",
          "command": "whichkey.triggerKey"
        }
      ]
    }
  ],
  "vim.visualstar": true,
  "vsicons.dontShowNewVersionMessage": true,
  "vsicons.presets.foldersAllDefaultIcon": true,
  "vsicons.projectDetection.autoReload": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "toggle",
  "window.openFilesInNewWindow": "on",
  "window.restoreWindows": "folders",
  "window.title": "${dirty}[${rootName}] ${activeEditorMedium}${separator}${appName}",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#252526",
    "statusBar.border": "#383838",
    "statusBar.debuggingBackground": "#b8bb26",
    "statusBar.foreground": "#ccc",
    "statusBar.noFolderBackground": "#b30000"
  },
  "workbench.colorTheme": "Gruvbox Dark (Medium)",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.showTabs": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true
}
